{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi g tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@apollo/client": "^3.4.16",
    "@umijs/preset-react": "^1.8.30",
    "antd": "^4.20.2",
    "graphql": "^15.6.1",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.23"
  },
  "devDependencies": {
    "@babel/traverse": "7.17.9",
    "@types/antd": "^1.0.0",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.0",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/plugin-utils": "^1.1.0",
    "@umijs/test": "^3.5.23",
    "autoprefixer": "^10.4.7",
    "lint-staged": "^10.0.7",
    "postcss": "^8.4.13",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.2.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
